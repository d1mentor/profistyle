@media (max-width: 1023px) {
	.header {
		max-height: 100%;
		background-color: #000;
		position: fixed;
		width: 100%;
		&__row {
			display: flex;
			flex-direction: column;
			justify-content: space-between;
			position: relative;
		}

		&__rank {
			display: flex;
			justify-content: space-between;
			position: relative;
			padding: 10px;
		}

		&__logo {
			display: block;
		}

		&__body {
			display: none;
		}
	}
	.menu-btn {
		display: flex; /* используем flex для центрирования содержимого */
		align-items: center; /* центрируем содержимое кнопки */
		justify-content: center;
		width: 40px;
		height: 40px;
		cursor: pointer;
		z-index: 1;
		padding: 5px 10px;
		border: 1px solid #fff;
		border-radius: 2px;
		transition: all 0.3s ease 0s;
	}
	/* добавляем "гамбургер" */
	.menu-btn > span,
	.menu-btn > span::before,
	.menu-btn > span::after {
		display: block;
		position: absolute;
		width: 20px;
		height: 5px;
		background-color: #fff;
	}
	.menu-btn > span::before {
		content: "";
		top: -8px;
	}
	.menu-btn > span::after {
		content: "";
		top: 8px;
	}

	.menu-btn:hover {
		border: 1px solid #007acc;
	}
	.menu-btn.active {
		border: none;
		background-color: #1a1a1a;
	}
	.menu {
		display: block;
		max-height: 0;
		overflow: hidden;
	}
	.menu.active {
		padding: 20px 0;
		max-height: 100%;
	}
	.nav {
		display: block;
		padding: 0 10px;
	}
	.nav a {
		color: #fff;
		font-size: 12px;
		font-weight: 700;
		transition: all 0.3s ease 0s;
	}
	.nav li {
		padding: 10px 0px;
	}
	.nav li:nth-of-type(1) {
		border-top: 1px solid #d1d1d1;
		border-bottom: 1px solid #d1d1d1;
	}
	.nav li {
		border-bottom: 1px solid #d1d1d1;
		position: relative;
	}
	.nav a:hover {
		color: #0fa89d;
	}
}
.collapsible.active::before {
	transform: rotate(-315deg);
}
.collapsible.active::after {
	transform: rotate(315deg);
}
.collapsible::before {
	content: "";
	position: absolute;
	bottom: 50%;
	left: 10px;
	width: 10px;
	height: 3px;
	background: #fff;
	transition: all 0.3s ease 0s;
	transform: rotate(-45deg);
}
.collapsible::after {
	content: "";
	position: absolute;
	bottom: 50%;
	right: 10px;
	width: 10px;
	height: 3px;
	background: #fff;
	transition: all 0.3s ease 0s;
	transform: rotate(45deg);
}
.collapsible {
	background: none;
	display: block;
	position: absolute;
	right: 0;
	top: 0;
	width: 25px;
	height: 36px;
}
.collapsible-content {
	padding: 0px 10px;
	color: #a1a1a1;
	max-height: 0;
	overflow: hidden;
	transition: all 0.3s ease 0s;
	display: flex;
	flex-direction: column;
	gap: 5px;
}
.collapsible-content a {
	border-bottom: 1px solid #a1a1a1;
	padding: 5px;
}
