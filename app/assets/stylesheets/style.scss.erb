//jaw
.jaw {
	position: fixed;
	top: 44px;
	width: 100%;
	z-index: 55;

	&__row {
		display: flex;
		justify-content: space-between;
		align-items: center;
		min-height: 80px;
		padding: 0 30px;
	}

	&__pic {
	}

	&__list {
		display: flex;
		align-items: center;
	}

	&__el:nth-of-type(3) {
		padding: 9px 2.25em 9px 12px;
	}
	&__el:nth-of-type(3) span,
	&__el:nth-of-type(3) span::before,
	&__el:nth-of-type(3) span::after {
		display: block;
		position: absolute;
		transition: all 0.3s ease 0s;
	}
	&__el:nth-of-type(3) span {
		top: 0;
		right: 12px;
		width: 6px;
		height: 6px;
	}
	&__el:nth-of-type(3) span::before {
		width: 10px;
		height: 3px;
		content: "";
		background: #fff;
		bottom: -12px;
		left: 0.5px;
		transform: rotate(-45deg);
	}
	&__el:nth-of-type(3) span::after {
		width: 10px;
		height: 3px;
		content: "";
		bottom: -12px;
		right: 0.5px;
		background: #fff;
		transform: rotate(45deg);
	}

	&__el:nth-of-type(3):hover span::after {
		transform: rotate(315deg);
	}
	&__el:nth-of-type(3):hover span::before {
		transform: rotate(-315deg);
	}
	&__el {
		position: relative;
		padding: 9px 12px;
		font-weight: 700;
		font-size: 14px;
		transition: all 0.3s ease 0s;
	}
	&__el > &__link.active {
		color: #0fa89d;
	}
	&__el > &__link {
		color: #fff;
		transition: all 0.3s ease 0s;
	}

	&__poster {
		display: none;
		transition: all 0.3s ease 0s;
		border: 1px solid rgba(51, 51, 51, 0.5);
	}

	&__poster > &__link {
		font-size: 14;
		font-weight: 700;
		color: #333;
		padding: 10px;
		border-bottom: 1px solid #333;
		transition: all 0.3s ease 0s;
	}
	&__link:nth-of-type(3) {
		border: none;
	}
	&__el:hover > &__link {
		color: #0fa89d;
	}
	&__poster > &__link:hover {
		color: #0fa89d;
	}
	&__el:hover &__poster {
		display: flex;
		flex-direction: column;
		position: absolute;
		top: 80%;
		right: 0px;
		width: 200%;
		background-color: #fff;
	}
	&__el:hover &__poster::after {
		content: "";
		display: block;
		position: absolute;
		width: 0;
		height: 0;
		border-bottom: 8px solid #fff;
		border-left: 8px solid transparent;
		border-right: 8px solid transparent;
		right: 6px;
		top: -8px;
	}
}

.sticky {
	position: fixed;
	top: 0;
	width: 100%;
	z-index: 55;
	background-color: rgba(0, 0, 0, 0.75);
}
.bg {
	background-color: rgba(0, 0, 0, 0.75);
}
@media (max-width: 1023px) {
	.jaw {
		display: none;
	}
