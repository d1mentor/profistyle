//roll
.roll {
	&__row {
		font-family: sans-serif;
		max-width: 840px;
		width: 100%;
		display: flex;
		margin: 0 auto;
		margin-top: -100px;
		padding: 0px 0px 90px 0px;
	}
	&__coll {
		flex: 0 0 33.333%;
		position: relative;
		transition: all 0.3s ease 0s;
		background-color: #fff;
		z-index: 1;
		padding: 30px 30px;
		box-shadow: 0px 5px 25px 0px rgba(0, 0, 0, 0.1);
	}
	&__coll::before,
	&__coll::after {
		content: "";
		position: absolute;
		width: 100%;
		height: 100%;
		top: 0;
		left: 0;
	}
	&__coll::before {
		transition: all 1.3s linear 0s;
	}
	&__coll::after {
		z-index: -1;
	}
	&__coll:hover::after {
		box-shadow: inset 1000px 0 0 0 rgba(6, 34, 65, 0.8);
		-webkit-transition: box-shadow 0.9s ease-out;
		-moz-transition: box-shadow 0.9s ease-out;
		-o-transition: box-shadow 0.9s ease-out;
		transition: box-shadow 0.9s ease-out;
	}
	&__coll::before.roll__coll-1 {
		z-index: -2;
		background: url("<%= asset_path('banner1.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__coll::before.roll__coll-2 {
		z-index: -2;
		background: url("<%= asset_path('banner2-2.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__coll::before.roll__coll-3 {
		z-index: -2;
		background: url("<%= asset_path('banner3-1.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__coll:hover::before {
		opacity: 1;
	}
	&__coll:hover &__coll-title {
		color: #fff;
	}
	&__coll:hover &__coll-text {
		color: #fff;
	}
	&__coll-title {
		color: #212529;
		font-size: 20px;
		font-weight: 600;
		padding: 0px 0px 45px 0px;
		transition: all 0.3s ease 0s;
	}
	&__coll-text {
		font-size: 16px;
		letter-spacing: 0.025em;
		line-height: 1.5;
		color: #7a7a7a;
		transition: all 0.3s ease 0s;
	}
}
