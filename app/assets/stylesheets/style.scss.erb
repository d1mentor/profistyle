//solutions
.solutions {
	&__row {
		display: flex;
		flex-direction: column;
		padding: 100px 40px;
		gap: 50px;
	}

	&__label {
		color: #212529;
		font-size: 30px;
		font-weight: 700;
		text-transform: uppercase;
		letter-spacing: 2px;
		font-family: sans-serif !important;
		text-align: center;
	}

	&__rank {
		display: flex;
		flex-wrap: wrap;
		justify-content: center;
	}

	&__el {
		flex: 0 0 33.333%;
		min-width: 350px;
		position: relative;
		font-family: sans-serif !important;
		padding: 60px 30px;
		transition: all 0.3s ease 0s;
		z-index: 0;
		box-shadow: 0px 5px 25px 0px rgba(0, 0, 0, 0.1);
		background-color: #fff;
	}
	&__el::after,
	&__el::before {
		content: "";
		position: absolute;
		width: 100%;
		height: 100%;
		top: 0;
		left: 0;
		transition: all 0.3s ease 0s;
	}
	&__el::after {
		z-index: -1;
		box-shadow: inset 0 0 0 0 rgba(6, 34, 65, 0.8);
	}
	&__el:hover::after {
		box-shadow: inset 1000px 0 0 0 rgba(6, 34, 65, 0.8);
	}
	&__el::before {
		z-index: -2;
	}
	&__el::before.sol__el-1 {
		background: url("<%= asset_path('banner1.jpg') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el::before.sol__el-2 {
		background: url("<%= asset_path('banner2-2.jpg') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el::before.sol__el-3 {
		background: url("<%= asset_path('banner3-1.jpg') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el::before.sol__el-4 {
		background: url("<%= asset_path('pr1.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el::before.sol__el-5 {
		background: url("<%= asset_path('pr3.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el::before.sol__el-6 {
		background: url("<%= asset_path('pr4.webp') %>") 0 0 / cover no-repeat;
		opacity: 0;
	}
	&__el:hover::before {
		opacity: 1;
	}

	&__el:hover &__el-title {
		color: #fff;
	}
	&__el:hover &__el-text {
		color: #fff;
	}

	&__el-label {
		transition: all 0.3s ease 0s;
		color: #0fa89d;
		font-size: 40px;
		font-weight: 700;
		padding: 0px 0px 10px 0px;
	}

	&__el-title {
		transition: all 0.3s ease 0s;
		color: #212529;
		font-size: 20px;
		padding: 0px 0px 10px 0px;
	}

	&__el-text {
		transition: all 0.3s ease 0s;
		color: #7a7a7a;
		font-family: "Roboto";
		font-weight: 400;
		font-size: 16px;
		letter-spacing: 0.025em;
		line-height: 1.5;
	}
	&__el-text a {
		color: #0fa89d;
	}
}

@media (max-width: 960px) {
	.solutions {
		&__row {
			gap: 0;
			padding: 100px 10px;
		}

		&__label {
			margin: 0px 0px 50px 0px;
		}

		&__rank {
			flex-direction: column;
		}

		&__el {
			min-width: 100%;
			flex: 1 1 auto;
			box-shadow: 0px 5px 15px 0px rgba(0, 0, 0, 0.05);
		}

		&__el::after,
		&__el::before {
			display: none;
		}
		&__el:hover &__el-title {
			color: #212529;
		}
		&__el:hover &__el-text {
			color: #7a7a7a;
		}
	}
}
/*--*/
